{% for name, vnet in vnets.items() %}
{% if vnet['vnet_network'] is defined %}

module "{{ name }}-vpc" {
    source                              = "{{ module_dir }}/vpc"
    providers                           = { aws = "aws.{{ vnet['vnet_network']['region'] }}"}
    create_vpc                          = true   
    name                                = "{{ name }}-vpc"   
    cidr                                = "{{ vnet['vnet_network']['network'] }}"
    assign_generated_ipv6_cidr_block    = false
}

{% endif %}
{% endfor %}